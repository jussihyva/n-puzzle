	"version": "0.2.0",
	"configurations":
	[
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/n_puzzle",
			// "args": ["-L", "2", "-rA", "dfs_1", "<", "data/3_3_01.map"],
			// "args": ["-L", "2", "-A", "dfs_3", "<", "data/3_3_03.map"],
			"args": ["-L", "2", "-A", "bfs_1", "<", "data/3_3_03.map"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}
